                <a class="sidebar-brand d-flex align-items-center justify-content-center" href="{{ path('app_dashboard') }}">
                    <div class="sidebar-brand-icon ">
                        <img src="/images/EMPSYS_logo.png" alt="Logo" style="width: 40px; height: 40px; border-radius: 10%;">
                    </div>
                    <div class="sidebar-brand-text mx-3">EMPSYS</div>
                </a>

                <!-- Divider -->
                <hr class="sidebar-divider my-0">

                <!-- Nav Item - Dashboard -->
                <li class="nav-item active">
                    <a class="nav-link" href="{{ path('app_dashboard') }}">
                        <i class="fas fa-fw fa-tachometer-alt"></i>
                        <span>Dashboard</span></a>
                </li>

                <!-- Divider -->
                <hr class="sidebar-divider">

                <!-- Heading -->
                <div class="sidebar-heading">Interface</div>

                <!-- Nav Item - Clientes Collapse Menu -->
                <li class="nav-item">
