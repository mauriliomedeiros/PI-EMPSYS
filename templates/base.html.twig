                       aria-expanded="true" aria-controls="collapseClientes">
                        <i class="fa fa-users"></i>
                        <span>Clientes</span>
                    </a>
                    <div id="collapseClientes" class="collapse" aria-labelledby="headingClientes" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Opções:</h6>
                            <a class="collapse-item" href="{{ path('cliente_new') }}">Adicionar</a>
                            <a class="collapse-item" href="{{ path('cliente_index') }}">Listar</a>
                        </div>
                    </div>
                </li>

                <!-- Nav Item - Locais Collapse Menu -->
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseLocais"
                       aria-expanded="true" aria-controls="collapseLocais">
                        <i class="fa fa-globe"></i>
                        <span>Locais</span>
                    </a>
                    <div id="collapseLocais" class="collapse" aria-labelledby="headingLocais"
                         data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Opções:</h6>
                            <a class="collapse-item" href="{{ path('local_new') }}">Adicionar</a>
                            <a class="collapse-item" href="{{ path('local_index') }}">Listar</a>
                        </div>
                    </div>
                </li>

                <!-- Nav Item - Maquinas Collapse Menu -->
                <li class="nav-item">
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#collapseMaquinas"
                       aria-expanded="true" aria-controls="collapseMaquinas">
                        <i class="fas fa-fw fa-cog"></i>
                        <span>Máquinas</span>
                    </a>
                    <div id="collapseMaquinas" class="collapse" aria-labelledby="headingMaquinas" data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <h6 class="collapse-header">Opções:</h6>
                            <a class="collapse-item" href="#">Fabricante</a>
                            <a class="collapse-item" href="#">Modelo</a>
                            <a class="collapse-item" href="#">Inventário</a>
                            <a class="collapse-item" href="{{ path('lista_empilhadeira_new') }}">Adicionar</a>
                            <a class="collapse-item" href="{{ path('lista_empilhadeira_index') }}">Listar</a>
