{% extends 'base.html.twig' %}

{% block title %}Empilhadeira{% endblock %}

{% block body %}
    <style>
        .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
        .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    </style>
    <hr>
    <div class="row">
        <div class="col-md-12">
            <h4 class="text-center">EMPILHADEIRA: {{ empilhadeira.fabricante }} {{ empilhadeira.modelo }}</h4>
        </div>
    </div>
    <hr>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ empilhadeira.id }}</td>
            </tr>
            <tr>
                <th>Fabricante</th>
                <td>{{ empilhadeira.fabricante }}</td>
            </tr>
            <tr>
                <th>Modelo</th>
                <td>{{ empilhadeira.modelo }}</td>
            </tr>
            <tr>
                <th>CargaTotal</th>
                <td>{{ empilhadeira.cargaTotal }}</td>
            </tr>
            <tr>
                <th>Caracteristicas</th>
                <td>{{ empilhadeira.caracteristicas }}</td>
            </tr>
            <tr>
                <th>Aplicacoes</th>
                <td>{{ empilhadeira.aplicacoes }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_empilhadeira_index') }}">back to list</a>

    <a href="{{ path('app_empilhadeira_edit', {'id': empilhadeira.id}) }}">edit</a>

    {{ include('empilhadeira/_delete_form.html.twig') }}
{% endblock %}
